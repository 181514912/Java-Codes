package hw04;

public class Hourly implements Salary{
private int balance;
private int hours;

public Hourly(int balance, int hours) {
	super();
	this.balance = balance;
	this.hours = hours;
}
public int getBalance() {
	return balance;
}
public void setBalance(int balance) {
	this.balance = balance;
}
public int getHours() {
	return hours;
}
public void setHours(int hours) {
	this.hours = hours;
}
public void salary() {
	hours+=(int)+0;
	balance=hours*40;
	System.out.println(hours+" годин(и) "+ balance+" гривень ");
}
@Override
public String toString() {
	return "Я заробив " + balance + " гривень , за " + hours + " годин(и)";
}

}
